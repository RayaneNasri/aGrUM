reward (typeneeded (highq (connected (good (apainted (good (bpainted (good (10.0))
                                                                     (poor (3.0))
                                                                     (f (3.0))))
                                                     (poor (3.0))
                                                     (f (3.0)) ))
                                     (bad (0.0))
                                     (f (0.0))))
                   (lowq (connected (good (apainted (good (bpainted (good (1.0))
                                                                    (poor (2.0))
                                                                    (f (1.0))))
