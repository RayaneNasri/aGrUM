import ADD;

system exemple_ADD {

  Leaf be1 (lambda= 0.000002);
  Leaf be2 (lambda= 0.000002, t= 8760);
  Leaf be3 (lambda= 0.000001, t= 8760);
  Leaf be4 (lambda= 0.0000002, t= 8760);
  Leaf be5 (lambda= 0.0000002, t= 8760);


  AndGate g1;
  g1.inputs += be1;
  g1.inputs += be2;

  AndGate g2;
  g2.inputs += be2;
  g2.inputs += be3;

  k_nGate g3;
  g3.inputs += be3;
  g3.inputs += be4;
  g3.inputs += be5;

  OrGate top ;
